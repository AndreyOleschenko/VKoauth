[MASTER]
ignore=.venv,migrations
ignore-patterns=test_.*\.py
load-plugins=pylint_django
django-settings-module=VKoauth.settings
jobs=1

[MESSAGES CONTROL]
disable=
    missing-docstring,
    invalid-name,
    too-few-public-methods,
    import-outside-toplevel,
    broad-exception-caught

[FORMAT]
max-line-length=120
